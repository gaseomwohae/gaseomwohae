<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gaseomwohae.gaseomwohae.repository.SupplyRepository">
    <select id="findAll" resultType="Supply">
        SELECT * FROM supply
    </select>

    <select id="findById" parameterType="Long" resultType="Supply">
        SELECT * FROM supply WHERE id = #{id}
    </select>
</mapper>